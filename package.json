{
  "version": "0.0.0",
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env TAILWIND_MODE=watch vite --host --port 5000",
    "build": "cross-env TAILWIND_MODE=build vite build",
    "serve": "vite preview",
    "export:video": "gsap-video-export http://localhost:5000 --output ./exports/animation.mp4 --fps 60",
    "export:video:hd": "gsap-video-export http://localhost:5000 --output ./exports/animation-1080.mp4 --width 1920 --height 1080 --fps 60",
    "export:video:4k": "gsap-video-export http://localhost:5000 --output ./exports/animation-4k.mp4 --width 3840 --height 2160 --fps 60",
    "export:gif": "ffmpeg -i ./exports/animation.mp4 -vf \"fps=30,scale=480:-1:flags=lanczos,split[s0][s1];[s0]palettegen[p];[s1][p]paletteuse\" -loop 0 ./exports/animation.gif"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.57.1",
    "vitawind": "^2.2.4",
    "vite": "^2.4.4"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "headroom.js": "^0.12.0",
    "netlify-cli": "^18.0.2",
    "onchange": "^7.1.0",
    "swiper": "^9.2.3",
    "three": "^0.154.0"
  }
}
